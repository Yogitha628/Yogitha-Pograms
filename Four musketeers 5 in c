#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x1,y1,x2,y2,x3,y3;
    scanf("%d%d%d%d%d%d",&x1,&y1,&x2,&y2,&x3,&y3);
    float a=(float)(x1+x2+x3)/3;
    float b=(y1+y2+y3)/3.0;
    printf("%.1f\n%.1f",a,b);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
