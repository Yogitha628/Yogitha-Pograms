import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int c=a+b;
        int d=a*b;
        if(d>1000)
        {
            System.out.println(c);
        }
        else
        {
            System.out.println(d);
        }
    }
}
