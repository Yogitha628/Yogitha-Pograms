import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
    while(n!=0)
    {
        int i=n%10;
        n=n/10;
        if(i%2!=0){
            sum=sum+i;
        }        
    }
    System.out.printf("%d",sum);
    }
}
