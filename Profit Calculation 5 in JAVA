import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int x,a,b;
        x=s.nextInt();
        a=s.nextInt();
        b=s.nextInt();
        int cp=x*b;
        int sp=x*a;
        int t=(sp-cp)-100;
        System.out.println(t);
    }
}
