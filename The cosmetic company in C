#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,r;
    scanf("%d",&n);
    int min=10;
    while(n!=0)
    {
        r=n%10;
        if(r<min)
        {
            min=r;
        }
        n=n/10;
    }
    printf("%d",min);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
