import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int sum=0;
    for(int i=a;i<=b;i++)
     sum= sum+i;  
    {
        System.out.printf("%d",sum);
    }

    }
}
