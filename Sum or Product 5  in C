#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b;
    scanf("%d%d",&a,&b);
    int p=a*b;
    int s=a+b;
    if(p>1000)
    {
        printf("%d",s);
    }
    else
    {
        printf("%d",p);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
